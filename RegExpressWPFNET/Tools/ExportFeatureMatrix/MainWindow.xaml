<Window x:Class="ExportFeatureMatrix.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExportFeatureMatrix"
        mc:Ignorable="d"
        Title="MainWindow" Height="188" Width="606">

    <Window.Resources>
        <Style TargetType="Button">
            <Setter Property="Padding" Value="8 4"/>
        </Style>
    </Window.Resources>
    
    <Grid>
        <Button x:Name="buttonExportExcel" Content="Create Excel file" HorizontalAlignment="Left" Margin="55,50,0,0" VerticalAlignment="Top" Click="buttonExportExcel_Click"/>

    </Grid>
</Window>
